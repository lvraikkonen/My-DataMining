library(class)
A1=c(0,0)
A2=c(1,1)
A3=c(2,2)
# Class B cases
B1=c(6,6)
B2=c(5.5,7)
B3=c(6.5,5)
# Build the classification matrix
train=rbind(A1,A2,A3, B1,B2,B3)
# Class labels vector (attached to each class instance)
cl=factor(c(rep("A",3),rep("B",3)))
# Plot
plot(train)
test=c(4, 4)
summary(knn(train, test, cl, k = 1))
